{% extends "layout.twig" %}

{% block css %}
{{ parent() }}
<link type="text/css" rel="stylesheet" href="/css/themes/ir_black.css">
<link type="text/css" rel="stylesheet" href="http://fonts.googleapis.com/css?family=Source+Code+Pro">
{% endblock %}

{% block javascript %}
<script src="/js/zepto.min.js"></script>
<script src="/js/highlight.min.js"></script>
<script>hljs.initHighlightingOnLoad();</script>
{% endblock %}

{% block content %}
<div class="paste">
  <pre><code>{{ paste.content | e }}</code></pre>
</div>
<!-- Digest: {{ paste.digest }} -->
<ul id="pasteOptions">
  <li><a href="/p/{{ id | e }}/raw">Raw</a></li>
  <li><a href="/p/{{ id | e }}/download">Download</a></li>
  <li><a href="/p/{{ id | e }}/dupe">Dupe</a></li>
</ul>
{% endblock %}
